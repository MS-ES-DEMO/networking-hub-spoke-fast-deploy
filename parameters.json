{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westeurope"
        },
        "tags": {
            "value": {
                "project": "fd-network-fundamentals"
            }
        },
        "resourceGroupName": {
            "value": "rg-network-fundamentals"
        },
        "hubVnetInfo": {
            "value": {
                "name": "vnet-hub",
                "range": "10.0.1.0/24"
            }
        },
        "onPremVNetInfo": {
            "value": {
                "name": "vnet-onpremises",
                "range": "10.0.0.0/24",
                "subnets": [
                    {
                        "name": "NetworkVirtualAppliances",
                        "range": "10.0.0.0/28"
                    },
                    {
                        "name": "GatewaySubnet",
                        "range": "10.0.0.240/28"
                    }
                ],
                "vpnGatewayName": "gw-vpn-onpremises",
                "vms": {
                    "vmOnPremises": {
                        "name": "vm-onpremises",
                        "sku": "Standard_DS2_V2",
                        "nicName": "nic-vm-onpremises",
                        "adminUsername": "azureAdmin"
                    }
                }
            }
        },
        "spoke1VNetInfo": {
            "value": {
                "name": "vnet-spoke1",
                "range": "10.0.2.0/24"
            }
        },
        "privateLinkVNetInfo": {
            "value": {
                "name": "vnet-privatelink",
                "range": "192.168.0.0/24"
            }
        }
    }
}