            "value": "westeurope"
        },
        "tags": {
            "value": {
                "project": "fd-network-fundamentals"
            }
        },
        "resourceGroupName": {
            "value": "rg-0"
        },
        "hubVnetConfiguration": {
            "value": {
                "name": "vnet-hub",
